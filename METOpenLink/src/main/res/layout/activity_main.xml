<?xml version="1.0" encoding="utf-8"?>

<com.milwaukeetool.mymilwaukee.view_reuseable.MTLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mt_white">

    <com.astuetz.PagerSlidingTabStrip
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/mainActivityTabs"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/mt_white"
        app:pstsTabBackground="@drawable/tab_bg"/>

    <android.support.v4.view.ViewPager
        android:id="@+id/mainActivityPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/mainActivityTabs"
        android:background="@color/mt_white"
        tools:context=".MainActivity" />

</com.milwaukeetool.mymilwaukee.view_reuseable.MTLayout>





